# Frontend Environment Variables
# Copy this file to .env and update the values as needed

# API URL for the frontend to connect to
# This should match your API server URL and CORS configuration
# IMPORTANT: Make sure this matches the CORS_ORIGINS in your API configuration
VITE_API_URL=http://localhost:1337

# App URL for generating links (optional)
VITE_APP_URL=http://localhost:3000

# Development Configuration
# Set to true to enable development features
VITE_DEV_MODE=false
